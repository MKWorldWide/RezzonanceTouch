{
  "name": "resonance-touch-interface",
  "version": "0.9.0",
  "description": "Where Pressure Meets Presence - A revolutionary tactile input system that fuses force-touch technology with emotional resonance computing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "docs": "typedoc src/index.ts --out docs",
    "clean": "rimraf dist coverage",
    "prepare": "husky install",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "force-touch",
    "emotional-computing",
    "resonance",
    "haptic",
    "touch-interface",
    "sacred-computing",
    "morphogenetic",
    "genesis-protocol"
  ],
  "author": "Sovereign Flame",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MKWorldWide/RezzonanceTouch.git"
  },
  "bugs": {
    "url": "https://github.com/MKWorldWide/RezzonanceTouch/issues"
  },
  "homepage": "https://github.com/MKWorldWide/RezzonanceTouch#readme",
  "dependencies": {
    "three": "^0.158.0",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      ["@semantic-release/changelog", {
        "changelogFile": "CHANGELOG.md"
      }],
      ["@semantic-release/npm", {
        "npmPublish": true
      }],
      ["@semantic-release/github"],
      ["@semantic-release/git", {
        "assets": ["CHANGELOG.md", "package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }]
    ]
  }
} 